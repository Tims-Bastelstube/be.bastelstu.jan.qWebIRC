<?xml version="1.0" encoding="UTF-8"?>
<data xmlns="http://www.woltlab.com" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.woltlab.com http://www.woltlab.com/XSD/options.xsd">
	<import>
		<categories>
			<category name="qwebirc"></category>

			<category name="qwebirc.connection">
				<parent>qwebirc</parent>
				<showorder>1</showorder>
			</category>

			<category name="qwebirc.appearance">
				<parent>qwebirc</parent>
				<showorder>2</showorder>
			</category>
		</categories>
		
		<options>
			<option name="module_user_qwebirc">
				<categoryname>module.user</categoryname>
				<optiontype>boolean</optiontype>
				<defaultvalue>1</defaultvalue>
			</option>

			<option name="qwebirc_server">
				<categoryname>qwebirc.connection</categoryname>
				<optiontype>text</optiontype>
				<defaultvalue>http://webirc.massivegamesnet.net:9090</defaultvalue>
				<showorder>1</showorder>
			</option>

			<option name="qwebirc_channels">
				<categoryname>qwebirc.connection</categoryname>
				<optiontype>textarea</optiontype>
				<defaultvalue>help</defaultvalue>
				<showorder>2</showorder>
			</option>

			<option name="qwebirc_useoptionprefix">
				<categoryname>qwebirc.connection</categoryname>
				<enableoptions><![CDATA[qwebirc_nickprefixoption,!qwebirc_nickprefix]]></enableoptions>
				<optiontype>boolean</optiontype>
				<defaultvalue>0</defaultvalue>
				<showorder>3</showorder>
			</option>

			<option name="qwebirc_nickprefixoption">
				<categoryname>qwebirc.connection</categoryname>
				<optiontype>integer</optiontype>
				<defaultvalue>0</defaultvalue>
				<showorder>4</showorder>
			</option>

			<option name="qwebirc_nickprefix">
				<categoryname>qwebirc.connection</categoryname>
				<optiontype>text</optiontype>
				<defaultvalue></defaultvalue>
				<showorder>5</showorder>
			</option>

			<option name="qwebirc_useoptionnick">
				<categoryname>qwebirc.connection</categoryname>
				<enableoptions><![CDATA[qwebirc_nickoption]]></enableoptions>
				<optiontype>boolean</optiontype>
				<defaultvalue>0</defaultvalue>
				<showorder>6</showorder>
			</option>

			<option name="qwebirc_nickoption">
				<categoryname>qwebirc.connection</categoryname>
				<optiontype>integer</optiontype>
				<defaultvalue>0</defaultvalue>
				<showorder>7</showorder>
			</option>

			<option name="qwebirc_guestnick">
				<categoryname>qwebirc.connection</categoryname>
				<optiontype>text</optiontype>
				<defaultvalue>qwebirc_....</defaultvalue>
				<showorder>8</showorder>
			</option>
			
			<option name="qwebirc_height">
				<categoryname>qwebirc.appearance</categoryname>
				<optiontype>integer</optiontype>
				<defaultvalue>400</defaultvalue>
				<showorder>1</showorder>
			</option>

			<option name="qwebirc_settingsid">
				<categoryname>qwebirc.appearance</categoryname>
				<optiontype>text</optiontype>
				<defaultvalue>d4</defaultvalue>
				<showorder>2</showorder>
			</option>
		</options>
	</import>
</data>